<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title><%- title %> Dashboard - User Detail</title>

    <%- include css-common %>
    <%- include js-common %>
    <script src="js/mustache.js"></script>
</head>
<body>
<!-- Facebook JavaScript -->
<div id="fb-root"></div>

<div data-role="page" id="users" class="type-menu-drilldown">
  <div data-role="header">
    <h1>Registered Users</h1>
  </div>

  <div data-role="content">
    <ul data-role="listview" data-inset="false" data-theme="c" data-filter="true" id="users-list"></ul>
  </div>
</div>


<!-- Login page -->
<div class="jumbotron">
    <div class="container">
        <div class="row well well-lg">
            <div class="col-md-6 col-md-offset-3 centerfy">
                <img src="images/logos/bang.on_128x128.png" alt="bang.on" class="img-responsive text-center"/>
            </div>
        </div>
    </div>
</div>

<script type="x-mustache-template" id="users_list_tmpl">
  {{#.}}
  <li data-icon="false" data-name="{{ fb.id }}"><a href="#" data-transition="slide"><img src="http://graph.facebook.com/{{ fb.id }}/picture"></img>{{ fb.name }}</a></li>
  {{/.}}
</script>

<script>
  $(document).ready(function(){

    var tmpl = $("#users_list_tmpl").html();
    var output = Mustache.to_html(tmpl, <%- users %>);
    $("#users-list").html(output).listview('refresh');
  });
</script>

</body>
</html>
